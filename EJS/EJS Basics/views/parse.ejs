<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>

  <body>
    <h1><%= title %></h1>
    <p>Current Seconds:<%= seconds %></p>
    <% if(seconds %2 === 0) { %>
    <p>even number of seconds</p>
    <% } else { %>
    <p>odd number of seconds</p>
    <% } %>
    <ul>
      <% for (let item of items){ %>
      <li><%= item %></li>
      <% } %>
    </ul>
    <div><%- htmlContent %></div>

    <div>
      <% if(locals.fakeItem){ %>
      <p>Fake item exists</p>
      <%}else{ %>
      <p>Fake item was not passed in</p>
      <%} %>
    </div>

    <section><%- include("footer.ejs") %></section>
  </body>
</html>
